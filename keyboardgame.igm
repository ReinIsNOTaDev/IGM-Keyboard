	name:backspace
	on click:
        gain 1 backspaceclick
        text=text-((floor(text/(pow(10,counter*2)/100))/100-floor(floor(text/(pow(10,counter*2)/100))/100))*100)*pow(10,counter*2)/100
    end
    on click:
        if (counter<32)
            lose 1 counter
        else 
            lose 2 counter
        end
            lose this
    end
*lettershift
	on click:
		if (have shifttoggle)
			lose shifttoggle
            lose this
		else
			gain 1 shifttoggle
	    end
    end
Includes
*include letter %l %c %a
  *letter[%l]
    tag:letter
    text:[?(have shifttoggle)|[%c]|[%l]]
    on click:
		$insertVar=[%a]
		  if (have shifttoggle) $insertVar=$insertVar+26
		  do insert with counter
		  if (counter>32) lose counter
      $textInsert=$insertVar*(pow(10,counter*2)/100)
      gain ($textInsert) text
      end
    on click:
        $insertVar=[%a]
        if (have shifttoggle) $insertVar=$insertVar+26
        if (no ach1) gain ($insertVar) fircli
    end
	on click:lose this
*include letterb %l %a %n
  *letter[%l]
    tag:letter
    name:[%n]
    on click:
		do insert with counter
		if (counter>32)
            toast Hit Maximum Letters.
        end
        $textInsert=$insertVar*(pow(10,counter*2)/10)
        gain ($textInsert) text
	end
	on click:lose this
*include texttemp %n
  *text[%n]
    is always:floor(text/(pow(10,[%n]*2)/100))
